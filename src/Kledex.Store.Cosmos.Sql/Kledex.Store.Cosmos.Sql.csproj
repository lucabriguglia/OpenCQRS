<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Description>CosmosDB (DocumentDB) database provider for Kledex domain store.</Description>
    <PackageTags>$(PackageTags), cosmosdb, sql, documentdb</PackageTags>
    <PackageLicenseExpression>$(PackageLicense)</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.DocumentDB.Core" Version="2.9.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Kledex\Kledex.csproj" />
  </ItemGroup>

</Project>
